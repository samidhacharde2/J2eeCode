<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean class="com.jspider.springcorexml.bean.StudentBean"
		name="student">
		<property name="id" value="101"></property>
		<property name="name" value="Abhi"></property>
		<property name="email" value="abhi@gamil.com"></property>
	</bean>

	<bean class="com.jspider.springcorexml.bean.StudentBean"
		name="student1">
		<property name="id" value="102" />
		<property name="name" value="Bhavika" />
		<property name="email" value="bhavika@gmail.com" />
	</bean>

	<bean class="com.jspider.springcorexml.bean.StudentBean"
		name="student2">
		<constructor-arg name="id" value="103" />
		<constructor-arg name="name" value="Chitra" />
		<constructor-arg name="email" value="chitraa@gmail.com" />
	</bean>

</beans>